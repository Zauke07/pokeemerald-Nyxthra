Route113_Text_SophiePostBattle:
	.string "Hollerididudljö!\p"
	.string "… …\p"
	.string "Jetzt DU.\n"
	.string "Sag Hollerididudljö!\n"
	.string "Regeln sind Regeln.$"


Route113_Text_LawrenceIntro:
	.string "Hollerididudljö!$"

Route113_Text_LawrenceDefeat:
